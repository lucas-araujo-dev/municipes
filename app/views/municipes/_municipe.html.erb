<div id="<%= dom_id municipe %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Full name:</strong>
    <%= municipe.full_name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Cpf:</strong>
    <%= municipe.cpf %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Cns:</strong>
    <%= municipe.cns %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Email:</strong>
    <%= municipe.email %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Birthdate:</strong>
    <%= municipe.birthdate %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Phone:</strong>
    <%= municipe.phone %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Picture:</strong>
    <%= municipe.picture %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Status:</strong>
    <%= municipe.status %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this municipe", municipe, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this municipe', edit_municipe_path(municipe), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
